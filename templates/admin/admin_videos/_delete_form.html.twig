<form method="post" action="{{ path('videos_delete', {'id': video.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ video.id) }}">
	<button class="btn btn-danger" style="margin-left:75%; margin-top:-5%">Supprimer</button>
</form>
